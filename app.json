{
  "expo": {
    "name": "Panela Velha",
    "slug": "tailwindPNV",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/splashlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/capizona.png",
      "resizeMode": "contain",
      "backgroundColor": "#fb923c"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splashlogo.png",
        "backgroundColor": "#fb923c"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/splashlogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "",
          "resizeMode": "contain",
          "backgroundColor": "#fb923c"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
